<addon>
    <displayName>Color Pickr</displayName>
    <version>0.1</version>
    <author>Matt L</author>
    <website>http://www.3villabs.com</website>
    <description>This tool will allow a user to chose which colors they would like to use in their skin</description>
    <customSettingsPage></customSettingsPage>
    <settings>
        <setting name="COLORPICKR_COLOR1" defaultValue="" type="text" displayName="Color 1"></setting>
        <setting name="COLORPICKR_COLOR2" defaultValue="" type="text" displayName="Color 2"></setting>
        <setting name="COLORPICKR_COLOR3" defaultValue="" type="text" displayName="Color 3"></setting>
    </settings>
</addon>